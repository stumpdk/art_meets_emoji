Check settings:
GET https://graph.facebook.com/v2.6/me/messenger_profile?fields=greeting,get_started,persistent_menu&access_token=ACCESS_TOKEN

Set settings:
POST

//thread_settings
{
  "setting_type" : "call_to_actions",
  "thread_state" : "new_thread",
  "call_to_actions":[
    {
      "type":"POSTBACK",
      "title":"Subscribe",
      "payload":"{\"type\": \"subscribe\"}"
    }
  ]
}

{
  "setting_type" : "call_to_actions",
  "thread_state" : "existing_thread",
  "call_to_actions":[
      {
        "type":"POSTBACK",
        "title":"Unsubscribe",
        "payload":"{\"type\": \"unsubscribe\"}"
      }
  ]
}


/messenger_profile
{"greeting":[
  {
    "locale":"default",
    "text":"Welcome to Art meets Emooji."
  }
]
}

//get_started
{
  "get_started":{
    "payload":"{\"type\": \"get_stated\"}"
  }
}
